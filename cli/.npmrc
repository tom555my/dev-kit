# npm configuration for dev-kit-cli

# Use provenance for verified publishing
provenance=true

# Save exact versions (optional, can be changed)
save-exact=false

# Engine strict - enforce Node.js version requirement
engine-strict=true

# Use pnpm as package manager (for CI/CD)
fund=false
audit=false

# provenance is only supported in public registries
# Remove this line if publishing to a private registry
